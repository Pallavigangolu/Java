class Shape 
{ 
 	String shapetype; 
 	Shape(String shapetype) 
 	{ 
 	 	this.shapetype = shapetype; 
 	} 
 	String dispShape() 
 	{ 
 	 	return shapetype; 
 	} 
 	double area() 
 	{ 
 	 	return 0.0; 
 	} 
} 
class Square extends Shape 
{ 
 	double side; 
 	Square(String shapetype,double area) 
 	{ 
 	 	super(shapetype); 
	 	 	this.side = side; 
	 	} 
	 	double area() 
	 	{ 
 
	 	 	return side*side; 
	 	} 
} 
class Rectangle extends Shape 
{ 
	 	double length,breadth; 
	 	Rectangle(String shapetype,double length,double breadth ) 
	 	{ 
 	 	super(shapetype);  	 	
    this.length = length;  	 
    this.breadth = breadth; 
	 	} 
	 	double area() 
	 	{ 
	 	 	return length*breadth; 
	 	} 
 } 
class RunTimePolDemo 
{ 
	 	public static void main(String[] args) 
	 	{ 
	 	 	Shape s = new Shape("NO SHAPE"); 
	 	 	Square sq = new Square("SQUARE",12.5); 
 	  	Rectangle r = new Rectangle("RECTANGLE",8.2,13.4);  	 
     Shape[] shp = {s,sq,r};  	 	for(Shape shobj : shp) 
	 	 	{ 
	 	 	 	System.out.println(shobj.dispShape() + "Area: " 
+ shobj.area()); 
	 	 	} 
	 	} 
} 

 
